package java_exam;

public class AritmaticOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 0;
		System.out.println(n);
		n++;
		System.out.println(n);
		
		System.out.println(n++);
		
		System.out.println(n);
		
		n = 0;
		System.out.println(n);
		n--;
		System.out.println(n);
		
		
		int m = 10;
		System.out.println(++m);
		System.out.println(m++);
		System.out.println(m);
		
		System.out.println(-mm);
		System.out.println(mm+);
		System.out.println(m);
	}

}
