package exam;

public class ArtithmaticProblem4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int subjA=100;
		int subjB=80;
		int subjC=95;
		int subjD=68;

		int totalScore=0;//  총합
		double average=0;// 평균점수
		double score =0; //4.5만점,학점계산,소수점2자리까지,(평균-55);
		
		totalScore = subjA+subjB+subjC+subjD;
		average= (double)totalScore/4;
		score = (average-55)*10;
		score = Math.floor(score);
		
		

		System.out.println(score*100);
	}

}
