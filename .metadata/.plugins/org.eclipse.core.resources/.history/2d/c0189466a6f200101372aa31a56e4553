package exam;

public class IfExam{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int randomNumber = (int) (Math.random()*100);
//		System.out.println(randomNumber);
		int radix = randomNumber / 10 * 10;
//		System.out.println(radix + "범위숫자나옴");
		
		
		if(randomNumber >= 10 && randomNumber<20) {
			System.out.println("10범위값입니다. 값은: ");
		}else if(randomNumber >= 20 && randomNumber<30) {
			System.out.println("20범위값입니다. 값은: ");
		}else if(randomNumber >= 30 && randomNumber<40) {
			System.out.println("30범위값입니다. 값은: ");
		}
		else if(randomNumber >= 40 && randomNumber<50) {
			System.out.println("40범위값입니다. 값은: ");
		}else {
			System.out.println("그외..등등..");
		}
		
		randomNumber = 78;
		if(randomNumber>=90) {
			System.out.println("90범위");
		}
	}

}
