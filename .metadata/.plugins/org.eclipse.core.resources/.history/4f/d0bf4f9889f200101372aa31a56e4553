package exam;

public class BjExam1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 1;
		int b = 2;
		
		System.out.println("Q1:"+(a+b));
		
		a=3;
		b=2;
		System.out.println("Q2:"+(a-b));
		
		a=1;
		b=3;
		System.out.println("Q3:"+((double)a/b));
		
		double c = 4.0;
		double d = 5.0;
		System.out.println("Q4:"+c/d);
		
	}

}
