package exam;

public class BjExam1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 1;
		int b = 2;
		
		System.out.println("Q1:"+(a+b));
		
		a=3;
		b=2;
		System.out.println("Q2:"+(a-b));
		
		a=1;
		b=3;
		System.out.println("Q3:"+((double)a/b));
		
		a=4;
		b=5;
		System.out.println("Q4:"+(double)(a/b)*100);
		
	}

}
