package exam;

public class ArtithmaticProblem2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int processTime = 145;
		int minutes = 0;
		int seconds = 0;
		
		minutes = (processTime*100)/60;

		seconds=processTime;
		
		System.out.println((float)minutes/100+"분 "+seconds+" 초");
		
	}

}
